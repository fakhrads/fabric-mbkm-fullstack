{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/1.1.0/schema.json",
  "global": {
    "fabricVersion": "2.3.3",
    "tls": false,
    "peerDevMode": false
  },
  "tools": { 
    "explorer": true 
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.unikom.ac.id"
      },
      "orderers": [
        {
          "groupName": "group1",
          "type": "solo",
          "instances": 1
        }
      ],
      "tools": {
        "fabloRest": true
      }
    },
    {
      "organization": {
        "name": "Org1",
        "domain": "org1.unikom.ac.id"
      },
      "peer": {
        "instances": 2,
        "db": "CouchDb"
      },
      "tools": {
        "fabloRest": true,
        "explorer": true
      }
    }
  ],
  "channels": [
    {
      "name": "my-channel1",
      "orgs": [
        {
          "name": "Org1",
          "peers": [
            "peer0",
            "peer1"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "chaincode1",
      "version": "0.0.1",
      "lang": "node",
      "channel": "my-channel1",
      "directory": "./chaincodes/registry-chaincode"
    }
  ]
}